    /* SVG DECLARTIONS */
    VAR _header = "data:image/svg+xml;utf8,"
    VAR _svgDeclaration = "<svg xmlns='http://www.w3.org/2000/svg'>"
    VAR _svgEnding = "</svg>" 
    
    /* CALCULATION */
    VAR _div =
        DIVIDE ( _actualQuantity, _previousQuantity ) - 1
