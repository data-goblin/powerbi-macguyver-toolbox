    VAR _negative = "#7D1D3A" // Negative Color
    VAR _filledCubeColor =
        // COLOR OF FILLED CUBES 
        SWITCH (
            TRUE (),
            _val >= 80, _positive,
            _val > 70, _neutral,
            _negative
        ) 
    
