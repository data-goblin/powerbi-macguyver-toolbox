              "",
              "-- Unicode characters",
              "VAR _FilledStar = UNICHAR( 9733 )         -- ★",
              "VAR _EmptyStar = UNICHAR( 9734 )          -- ☆",
              "-- Result",
              "VAR _StarRating =",
              "    REPT( _FilledStar, _FilledStarsCount )",
              "        & REPT( _EmptyStar, _EmptyStarsCount )",
              "RETURN",
              "    _StarRating & UNICHAR( 10 )"
            ],
