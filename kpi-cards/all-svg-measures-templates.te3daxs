    VAR _usableHeight = _height - _doublePadding // HEIGHT OF USABLE SPACE
    VAR _usableWidth = _width - _doublePadding // WIDTH OF USABLE SPACE
    VAR _imageSize =
        MIN ( _usableHeight, _usableWidth ) // HEIGHT SIZE OF IMAGE
    
